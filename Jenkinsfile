// declarative pipeline
pipeline{
    agent any
    stages{
        stage("stage 1"){
            steps{
                echo 'Hello World'
            }
        }
        stage("stage 2"){
            steps{
                echo "prueba" 
            }
        }
    }  
}

// scripted pipeline

// node { 
//     stage('Stage 1') {
//         echo 'Hello World'
//     }
//     stage("build"){
//         echo "prueba" 
//     }
// }
